{
  "id": null,
  "uid": "diet-api-monitoring",
  "title": "Diet Plan API Monitoring",
  "tags": ["diet-api", "flask", "gemini"],
  "style": "dark",
  "timezone": "browser",
  "editable": true,
  "graphTooltip": 1,
  "refresh": "5s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"]
  },
  "panels": [
    {
      "id": 1,
      "title": "API Request Rate",
      "type": "graph",
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "rate(flask_app_requests_total[5m])",
          "legendFormat": "{{method}} {{endpoint}} - {{http_status}}",
          "refId": "A"
        }
      ],
      "yaxes": [
        { "label": "Requests/sec", "format": "reqps", "show": true },
        { "show": false }
      ],
      "xaxis": { "show": true, "mode": "time" },
      "legend": {
        "show": true,
        "alignAsTable": true,
        "avg": true,
        "current": true
      },
      "tooltip": { "shared": true, "sort": 0 }
    },
    {
      "id": 2,
      "title": "Active Requests",
      "type": "stat",
      "gridPos": { "x": 12, "y": 0, "w": 6, "h": 4 },
      "targets": [
        {
          "expr": "sum(active_requests)",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 5, "color": "yellow" },
              { "value": 10, "color": "red" }
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "title": "API Initialization Status",
      "type": "stat",
      "gridPos": { "x": 18, "y": 0, "w": 6, "h": 4 },
      "targets": [
        {
          "expr": "api_initialization_status",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "orientation": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"]
        }
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "value": "0", "text": "Failed" },
            { "type": "value", "value": "1", "text": "Success" }
          ],
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "red" },
              { "value": 1, "color": "green" }
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "title": "Response Time (p50, p95, p99)",
      "type": "graph",
      "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(flask_app_request_latency_seconds_bucket[5m]))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, rate(flask_app_request_latency_seconds_bucket[5m]))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, rate(flask_app_request_latency_seconds_bucket[5m]))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "yaxes": [
        { "label": "Seconds", "format": "s", "show": true },
        { "show": false }
      ],
      "xaxis": { "show": true, "mode": "time" },
      "legend": {
        "show": true,
        "alignAsTable": true,
        "avg": true,
        "current": true
      },
      "tooltip": { "shared": true, "sort": 0 }
    },
    {
      "id": 5,
      "title": "Total Requests by Status Code",
      "type": "stat",
      "gridPos": { "x": 12, "y": 4, "w": 12, "h": 4 },
      "targets": [
        {
          "expr": "sum by (http_status) (flask_app_requests_total)",
          "legendFormat": "{{http_status}}",
          "refId": "A"
        }
      ],
      "options": {
        "orientation": "horizontal",
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 6,
      "title": "Diet Plan Generation Rate",
      "type": "graph",
      "gridPos": { "x": 0, "y": 16, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "rate(diet_plan_requests_total{status='success'}[5m])",
          "legendFormat": "Success - {{goal}} ({{diet_preference}})",
          "refId": "A"
        },
        {
          "expr": "rate(diet_plan_requests_total{status='failure'}[5m])",
          "legendFormat": "Failure - {{goal}} ({{diet_preference}})",
          "refId": "B"
        }
      ],
      "yaxes": [
        { "label": "Plans/sec", "format": "short", "show": true },
        { "show": false }
      ],
      "xaxis": { "show": true, "mode": "time" },
      "legend": {
        "show": true,
        "alignAsTable": true,
        "avg": true,
        "current": true
      },
      "tooltip": { "shared": true, "sort": 0 },
      "seriesOverrides": [{ "alias": "/Failure.*/", "color": "#E02F44" }]
    },
    {
      "id": 7,
      "title": "Diet Plan Generation Time",
      "type": "graph",
      "gridPos": { "x": 12, "y": 16, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(diet_plan_generation_seconds_bucket[5m]))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, rate(diet_plan_generation_seconds_bucket[5m]))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, rate(diet_plan_generation_seconds_bucket[5m]))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "yaxes": [
        { "label": "Seconds", "format": "s", "show": true },
        { "show": false }
      ],
      "xaxis": { "show": true, "mode": "time" },
      "legend": {
        "show": true,
        "alignAsTable": true,
        "avg": true,
        "current": true
      },
      "tooltip": { "shared": true, "sort": 0 }
    },
    {
      "id": 8,
      "title": "Success vs Failure Rate",
      "type": "piechart",
      "gridPos": { "x": 0, "y": 24, "w": 6, "h": 8 },
      "targets": [
        {
          "expr": "sum by (status) (diet_plan_requests_total)",
          "legendFormat": "{{status}}",
          "refId": "A"
        }
      ],
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "pieType": "pie",
        "tooltip": { "mode": "single" }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 9,
      "title": "Total Diet Plans Generated",
      "type": "stat",
      "gridPos": { "x": 6, "y": 24, "w": 6, "h": 4 },
      "targets": [
        {
          "expr": "sum(diet_plan_requests_total{status='success'})",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "center",
        "orientation": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "value": null, "color": "green" }]
          }
        }
      }
    },
    {
      "id": 10,
      "title": "Total Failures",
      "type": "stat",
      "gridPos": { "x": 6, "y": 28, "w": 6, "h": 4 },
      "targets": [
        {
          "expr": "sum(diet_plan_failures_total)",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "center",
        "orientation": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 1, "color": "yellow" },
              { "value": 10, "color": "red" }
            ]
          }
        }
      }
    },
    {
      "id": 11,
      "title": "Failures by Error Type",
      "type": "graph",
      "gridPos": { "x": 12, "y": 24, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "rate(diet_plan_failures_total[5m])",
          "legendFormat": "{{error_type}} - {{goal}}",
          "refId": "A"
        }
      ],
      "yaxes": [
        { "label": "Failures/sec", "format": "short", "show": true },
        { "show": false }
      ],
      "xaxis": { "show": true, "mode": "time" },
      "legend": {
        "show": true,
        "alignAsTable": true,
        "avg": true,
        "current": true
      },
      "tooltip": { "shared": true, "sort": 0 }
    },
    {
      "id": 12,
      "title": "Gemini API Calls",
      "type": "graph",
      "gridPos": { "x": 0, "y": 32, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "rate(gemini_api_calls_total[5m])",
          "legendFormat": "{{model_name}} - {{status}}",
          "refId": "A"
        }
      ],
      "yaxes": [
        { "label": "Calls/sec", "format": "short", "show": true },
        { "show": false }
      ],
      "xaxis": { "show": true, "mode": "time" },
      "legend": {
        "show": true,
        "alignAsTable": true,
        "avg": true,
        "current": true
      },
      "tooltip": { "shared": true, "sort": 0 }
    },
    {
      "id": 13,
      "title": "JSON Parse Errors",
      "type": "stat",
      "gridPos": { "x": 12, "y": 32, "w": 6, "h": 4 },
      "targets": [
        {
          "expr": "sum(json_parse_errors_total)",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "center",
        "orientation": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 5, "color": "yellow" },
              { "value": 20, "color": "red" }
            ]
          }
        }
      }
    },
    {
      "id": 14,
      "title": "User Profile Distribution (Goal)",
      "type": "piechart",
      "gridPos": { "x": 18, "y": 32, "w": 6, "h": 8 },
      "targets": [
        {
          "expr": "sum by (goal) (user_profile_requests)",
          "legendFormat": "{{goal}}",
          "refId": "A"
        }
      ],
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "pieType": "pie",
        "tooltip": { "mode": "single" }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 15,
      "title": "Diet Preference Distribution",
      "type": "bargauge",
      "gridPos": { "x": 0, "y": 40, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "sum by (diet_preference) (user_profile_requests)",
          "legendFormat": "{{diet_preference}}",
          "refId": "A"
        }
      ],
      "options": {
        "orientation": "horizontal",
        "displayMode": "gradient",
        "showUnfilled": true,
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "palette-classic" }
        }
      }
    },
    {
      "id": 16,
      "title": "Activity Level Distribution",
      "type": "bargauge",
      "gridPos": { "x": 12, "y": 40, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "sum by (activity_level) (user_profile_requests)",
          "legendFormat": "{{activity_level}}",
          "refId": "A"
        }
      ],
      "options": {
        "orientation": "horizontal",
        "displayMode": "gradient",
        "showUnfilled": true,
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "palette-classic" }
        }
      }
    },
    {
      "id": 17,
      "title": "Calorie Target Distribution",
      "type": "heatmap",
      "gridPos": { "x": 0, "y": 48, "w": 24, "h": 8 },
      "targets": [
        {
          "expr": "rate(calorie_target_distribution_bucket[5m])",
          "legendFormat": "{{le}}",
          "refId": "A",
          "format": "heatmap"
        }
      ],
      "dataFormat": "tsbuckets",
      "cards": {
        "cardPadding": null,
        "cardRound": null
      },
      "color": {
        "mode": "spectrum",
        "cardColor": "#b4ff00",
        "colorScale": "sqrt",
        "exponent": 0.5,
        "colorScheme": "interpolateOranges"
      },
      "legend": {
        "show": true
      },
      "yAxis": {
        "format": "short",
        "decimals": null,
        "logBase": 1,
        "show": true
      },
      "xAxis": {
        "show": true
      }
    },
    {
      "id": 18,
      "title": "Error Rate (%)",
      "type": "gauge",
      "gridPos": { "x": 12, "y": 36, "w": 6, "h": 4 },
      "targets": [
        {
          "expr": "100 * sum(rate(diet_plan_requests_total{status='failure'}[5m])) / sum(rate(diet_plan_requests_total[5m]))",
          "refId": "A"
        }
      ],
      "options": {
        "showThresholdLabels": false,
        "showThresholdMarkers": true,
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 5, "color": "yellow" },
              { "value": 10, "color": "red" }
            ]
          }
        }
      }
    },
    {
      "id": 19,
      "title": "Average Response Time (Last Hour)",
      "type": "stat",
      "gridPos": { "x": 18, "y": 36, "w": 6, "h": 4 },
      "targets": [
        {
          "expr": "avg(rate(flask_app_request_latency_seconds_sum[1h]) / rate(flask_app_request_latency_seconds_count[1h]))",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "center",
        "orientation": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 2,
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 2, "color": "yellow" },
              { "value": 5, "color": "red" }
            ]
          }
        }
      }
    }
  ],
  "schemaVersion": 36,
  "version": 1
}
